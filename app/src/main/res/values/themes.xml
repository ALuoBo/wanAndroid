<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimaryDark">@color/design_default_color_background</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowTranslucentStatus">true</item>

    </style>

    <style name="Theme.MyTheme" parent="AppTheme">
        <item name="colorPrimary">@color/colorAccent</item>
    </style>

    <style name="AppLauncherTheme" parent="AppTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/ic_method_draw_image</item>
    </style>
</resources>
